pipeline {
    agent {
        node {
            label 'master'
        }
    }

    stages {

        stage('terraform started') {
            steps {
                sh 'echo "Started...!" '
            }
        }
        stage('git clone') {
            steps {
                sh 'git clone https://github.com/send2durai/jenkins-1.git'
            }
        }
        stage('terraform init') {
            steps {
                sh 'C:\Users\tmurugan\Desktop\Terraform_Scripts\Single_EC2 provision_Ubuntu AMI/terraform init'
            }
        }
        stage('terraform plan') {
            steps {
                sh 'C:\Users\tmurugan\Desktop\Terraform_Scripts\Single_EC2 provision_Ubuntu AMI/terraform plan'
            }
        }
        stage('terraform ended') {
            steps {
                sh 'echo "Ended....!!"'
            }
        }

        
    }
}
